{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"allowJs": true,
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"components/home/recommended/Recommendedjobs.jsx",
		"app/job-details/detailItem.jsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app/(tabs)/profile.jsx",
		"app/(tabs)/notifications.jsx",
		"app/(tabs)/jobs.jsx",
		"app/(tabs)/feed.jsx",
		"app/(tabs)/applications.jsx",
		"app/(tabs)/_layout.jsx",
		"app/modal.jsx",
		"app/faq.jsx",
		"app/+html.jsx",
		"app/[...missing].jsx",
		"app/_layout.jsx"
	]
}
